<el-form ref='signInForm' :model='form' label-position='top' :rules='rules' @keyup.enter.native="handleSubmit">
	<el-form-item label='Email' prop='email'>
		<el-input v-model='form.email' placeholder='example@stevens.edu'></el-input>
	</el-form-item>

	<el-form-item label='Password' prop='password'>
		<el-input v-model='form.password' type='password' placeholder='Input your password'></el-input>
	</el-form-item>

	<el-form-item>
		<el-button type='primary' @click="handleSubmit">Sign In</el-button>
	</el-form-item>
</el-form>