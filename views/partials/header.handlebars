<header class='header-container flex'>
	<span class='header__logo'>
		<img src='../../public/static/logo_white.svg' alt='' />
	</span>
	<input class='form-control header__input' placeholder='Type to search' type='text' />
	{{#if logged}}
		<img class='header__avatar' src='../../public/static/avatar.png' alt='' />
	{{else}}
		<a class='aside-h btn btn-danger' href='/authorize/signup'>
			Sign up
		</a>
		<a class='aside-h btn btn-outline-danger' href='/authorize/signin'>
			Sign in
		</a>
	{{/if}}
</header>